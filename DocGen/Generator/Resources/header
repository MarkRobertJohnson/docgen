<script type="text/javascript">

$(document).ready(function()
{
 $("#TOC").hide(); //hide the table of contents
 
 //Add .prettyprint class to all 'pre' tags. This makes google-code-prettify work automatically on Markdown "code" output.
 //(see also http://www.codingthewheel.com/archives/syntax-highlighting-stackoverflow-google-prettify, but this version uses jQuery)
 $('pre').addClass('prettyprint');
 prettyPrint(); //call google-code-prettify
 
 /*Effects - toggle visibility of TOC*/
 $("#TOC-select").click(function()
 {
  fixed_slideToggle("#TOC"); /*see jquery-fix.js (repairs a known bug in jQuery core). Replace with below once core is fixed:
  /*$("#TOC").slideToggle("fast");
  $(this).toggleClass("active"); */
 });
 
 $(".button").hover(function()
 {
  this.src = this.src.replace("_off","_on");
 },
 function()
 {
  this.src = this.src.replace("_on","_off");
 }
);
 
});
</script>

